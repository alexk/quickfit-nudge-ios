# FitDad Nudge Environment Variables
# Copy this file to .env and fill in your values

# Development Environment
AMPLITUDE_API_KEY=your_amplitude_dev_key
CLOUDKIT_CONTAINER=iCloud.com.fitdad.nudge
BUNDLE_IDENTIFIER=com.fitdad.nudge
WATCHKIT_BUNDLE_IDENTIFIER=com.fitdad.nudge.watchkitapp
WIDGET_BUNDLE_IDENTIFIER=com.fitdad.nudge.widget
DEVELOPMENT_TEAM=YOUR_TEAM_ID

# App Store Connect
APP_STORE_CONNECT_KEY_ID=your_key_id
APP_STORE_CONNECT_ISSUER_ID=your_issuer_id

# Fastlane
MATCH_GIT_URL=https://github.com/your-org/certificates
MATCH_PASSWORD=your_match_password

# TestFlight
PILOT_APPLE_ID=your@email.com
PILOT_TEAM_ID=your_team_id 